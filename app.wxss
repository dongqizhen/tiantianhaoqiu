/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  width:100%;
  height: 100%;
  overflow-x:hidden;
  display: flex;
  flex-direction: column;
}
page .wrapper{
  display: flex;
  flex-direction: column;
}
.content{
  width:100%;
  overflow-x:hidden;
}
.noContent{
  text-align: center;
  padding-top: 100rpx;
}
.no_img{
  width: 250rpx;
  height: 160rpx;
  display: block;
  margin: 0 auto;
}
.no_text{
  color: #9A9FB4;
  font-size: 27rpx;
  padding-top: 40rpx;
}